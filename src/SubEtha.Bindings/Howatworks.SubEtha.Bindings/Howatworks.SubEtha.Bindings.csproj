<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Title>Elite: Dangerous Player Journal Parser</Title>
    <Description>Plain .NET object representations of control bindings for the game Elite: Dangerous</Description>
    <PackageTags>elite dangerous controls bindings</PackageTags>
    <Authors>James Howat</Authors>
    <PackageProjectUrl>https://github.com/johnnysaucepn/SubEtha</PackageProjectUrl>
    <RepositoryUrl>https://github.com/johnnysaucepn/SubEtha</RepositoryUrl>
    <PackageIconUrl>https://static.howatworks.com/thumb_purple_64.png</PackageIconUrl>
    <Configurations>Debug;Release</Configurations>
  </PropertyGroup>

</Project>
