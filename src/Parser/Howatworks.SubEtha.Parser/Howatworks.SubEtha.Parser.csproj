<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Title>Elite: Dangerous Player Journal Parser</Title>
    <Description>An API for quick and easy parsing of Player Journal log entries in the game Elite: Dangerous</Description>
    <PackageTags>elite dangerous player journal parser</PackageTags>
    <Authors>James Howat</Authors>
    <PackageProjectUrl>https://github.com/johnnysaucepn/SubEtha</PackageProjectUrl>
    <RepositoryUrl>https://github.com/johnnysaucepn/SubEtha</RepositoryUrl>
    <Configurations>Debug;Release</Configurations>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="System.Reactive" Version="5.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Journal\Howatworks.SubEtha.Journal\Howatworks.SubEtha.Journal.csproj" />
  </ItemGroup>

  <Import Project="..\..\NuGetAssets.props" />

</Project>
